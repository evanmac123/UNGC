<%= form_for @search, as: :search, url: library_search_path, method: :get, html: {class: 'title-search-link'} do |f| %>
  <fieldset id='section-search'>
    <%= f.search_field :keywords, placeholder: 'Title Search', tabindex: 1, autofocus: true %>
    <%= f.button '<span>Search</span>'.html_safe, type: :submit %>
  </fieldset>
<% end %>
