%tr
  %td= award.type_name
  %td= award.title
  %td= award.date
  %td= link_to_uploaded_file(award.file)
  - if current_contact_can_edit_network?
    %td
      %ul.actions
        %li= link_to 'Edit', edit_submodel_path(award), :class => 'edit', :title => 'Edit award details'
        %li= link_to 'Delete', submodel_path(award), :confirm => 'Are you sure you want to delete this award?', :method => :delete, :class => 'delete', :title => 'Delete Award'
