<h2>LEAD Companies - Latest COP Submission</h2>

<p>
  <ul class="tools inline">
    <li><%= link_to_download_report %></li>
  </ul>
</p>

<table class="dashboard_table">
  <tr>
    <% @report.headers.each do |header| %>
      <th><%= header %></th>
    <% end %>
    <th>Actions</th>
  </tr>
  <% @report.to_h.each do |row|  %>
    <tr>
      <% row.values.each do |col| %>
        <td><%= col %></td>
      <% end %>
      <td>
        <ul class="actions">
        </ul>
    </tr>
  <% end %>
</table>

<%= render partial: 'report_modal' %>
