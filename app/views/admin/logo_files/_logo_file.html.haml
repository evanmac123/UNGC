%li.item{:id => dom_id(logo_file)}
  .meta
    %p.name= logo_file.name
    - if @logo_request.can_download_files?
      = image_tag logo_file_image_url(logo_file)
      = link_to 'Download', download_admin_organization_logo_request_path(@logo_request.organization_id, @logo_request, :logo_file_id => logo_file.id)
