%h2 New contact

= form_for(:contact, builder: DefaultFormBuilder, url: contact_parent_path(@contact, :contacts)) do |form|
  = render :partial => form
  %ul{:class => "tools inline", :style => 'float: right;'}
    %li= link_to 'Cancel', @return_path, :class => 'cancel_large'
    %li= form.submit 'Create', :class => 'button', :style => 'float: right;'
