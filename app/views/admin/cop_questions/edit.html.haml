%h2 Editing COP question
= link_to 'Cancel and return to COP Questions', admin_cop_questions_path, :class => 'cancel_large'
- form_for @cop_question, builder: DefaultFormBuilder, url: admin_cop_question_path(@cop_question) do |form|
  = render :partial => form
  = form.submit 'Save changes', :class => 'button'
