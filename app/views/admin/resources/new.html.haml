%h2 New Resource

= form_for @resource, builder: DefaultFormBuilder, :url => admin_resources_path, :html => {:multipart => true} do |form|
  = render :partial => form

  %ul{:class => "tools inline", :style => 'float: right;'}
    %li= link_to 'Cancel', admin_resources_path, :class => 'cancel_large'
    %li= form.submit 'Create', :class => 'button'
