%h2 New Meeting

= form_for @submodel, builder: DefaultFormBuilder, url: submodels_path, html: {multipart: true, novalidate: "novalidate"} do |form|
  = render partial: form
  %ul{class: "tools inline", style: 'float: right;'}
    %li= link_to 'Cancel', return_url, class: 'cancel_large'
    %li= form.submit 'Create', class: 'button'
