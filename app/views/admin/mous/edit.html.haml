%h2 Edit Memorandum of Understanding

= form_for @submodel, builder: DefaultFormBuilder, url: submodel_path(@submodel), html: {multipart: true, novalidate: "novalidate"} do |form|
  = render partial: form
  %ul{class: "tools inline", style: 'float: right;'}
    %li= link_to 'Cancel', admin_local_network_path(@local_network, tab: @tab), class: 'cancel_large'
    %li= form.submit 'Save changes', class: 'button'
