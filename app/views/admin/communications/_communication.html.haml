%tr
  %td= communication.type_name
  %td= communication.title
  %td= communication.date
  %td= link_to_uploaded_file(communication.file)
  %td
    %ul.actions
      %li= link_to 'Edit', edit_submodel_path(communication), :class => 'edit', :title => 'Edit details'
      %li= link_to 'Delete', submodel_path(communication), :confirm => 'Are you sure you want to delete this communication?', :method => :delete, :class => 'delete', :title => 'Delete meeting'